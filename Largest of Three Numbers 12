int main() {
    int a, b, c;
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    if (a > b && a > c)
        printf("a is largest then b and c\n");
    else if (b > a && b > c)
        printf("b is largest then a and c\n");
    else if (c > a && c > b)
        printf("c is largest then a and b\n");
    else
        printf("Two or more numbers are equal and largest\n");
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
